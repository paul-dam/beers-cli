#!/usr/bin/env php<?phpvar_dump(__DIR__);if (is_file($autoload = __DIR__ . '/../vendor/autoload.php')) {    require($autoload);} else {    fwrite(STDERR, 'No autoload found');    exit(1);}if (is_file($bootstrap = __DIR__ . '/../bootstrap/app.php')) {    (new \PaulDam\BeersCli\Console\Application(require($bootstrap)))->run();} else {    fwrite(STDERR, 'No bootstrap found.');}